{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "UserID=postgres;Password=postgres;Host=db-main;Port=5432;Database=main;Pooling=true;MinPoolSize=0;MaxPoolSize=100;ConnectionLifetime=0;IncludeErrorDetail=true;"
  },
  "AWS": {
    "BucketName": "pathmaker",
    "Environment": "docker",
    "AccessKey": "<secret>",
    "SecretKey": "<secret>",
    "ServiceUrl": "https://s3.eu-north-1.amazonaws.com"
  },
  "FeatureFlags": {
    "DebugEndpoints": true,
    "SwaggerUi": true,
    "HangfireDashboard": true
  }
}
